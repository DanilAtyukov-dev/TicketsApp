<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OfferView">
        <attr name="img" format="integer" />
        <attr name="title" format="string" />
        <attr name="city" format="string"/>
        <attr name="price" format="string"/>
        <attr name="transport" format="integer"/>
    </declare-styleable>

    <declare-styleable name="DoubleSearchView">
        <attr name="mode" format="enum">
            <enum name="first" value="1"/>
            <enum name="second" value="2"/>
            <enum name="third" value="3"/>
        </attr>

        <attr name="visibilityLeftIcons" format="boolean"/>
        <attr name="visibilityReversIcon" format="boolean"/>
        <attr name="visibilityCloseIcon" format="boolean"/>
    </declare-styleable>
</resources>